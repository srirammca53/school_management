<h3> Your Respective batchmates </h3>

<table>
	<thead>
		<th>First Name</th>
		<th>Last Name</th>
		<th>Email</th>
		<th>City</th>
		<th>Phone</th>
	</thead>
  	<tbody>
      <% @users.each do |user| %>
  		<tr>
  			<td><%= user.first_name %></td>
  			<td><%= user.last_name %></td>
  			<td><%= user.email %></td>
  			<td><%= user.city %></td>
  			<td><%= user.phone %></td>
  		</tr>
  	  <% end %>
  	</tbody>
</table>


